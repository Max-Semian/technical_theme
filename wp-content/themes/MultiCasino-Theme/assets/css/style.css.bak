table {
  border-collapse: collapse; 
  width: 100%;
  background-color: #481A54;
  color: #fff;
  font-family: sans-serif;
  font-size: 16px;
  margin: 23px auto;
  border: 1px solid #1A0502; 
  border-radius: 4px;
  overflow: hidden;
}

  th, td {
    border: 1px solid #1A0502;
    padding: 10px 12px;
    text-align: left;
    vertical-align: top;
  }

  thead tr {
    background-color: #51255d;
  }

  th {
    font-weight: 600;
  }
  td {
    font-weight: 400;
  }

  tbody tr {
    background-color: #481A54;
  }

  .main-content .block-content table tr td {
  	border-left: none;
  	padding: 10px 12px;
  }



@media screen and (max-width: 1000px) {
  table {
		margin: 23px 0;
		display: block;
  overflow-x: auto;
  white-space: nowrap;
  -webkit-overflow-scrolling: touch;
	}
	th, td {
		width: 100%;
	}
	.main-content .block-content .group-content .text {
	width: 100%;
}
}

